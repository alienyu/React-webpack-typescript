{
  "compilerOptions": {
    "outDir": "./dist/",
    "baseUrl": ".",
    "paths": {
      "@style/*": [
        "page/web/ts/main/test/components/style/*"
      ],
      "@model/*": [
        "page/web/ts/main/test/model/*"
      ],
      "@static/*": [
        "page/web/ts/main/test/static/*"
      ]
    },
    "sourceMap": true,
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "lib": [
      "es2016",
      "dom"
    ],
  },
  "include": [
    "./page/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}